{
  "name": "l2",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:akoidan/l2.git",
  "author": "akoidan <deathangel908@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node -r tsconfig-paths/register -r ts-node/register src/main.ts",
    "prebuild": "rm -rf ./dist/ && nest build && cp src/auth/public_key.pem dist/auth/public_key.pem",
    "build": "pkg . --targets win --output app.exe",
    "wine": "wine app.exe",
    "native": "node-gyp rebuild && node -r tsconfig-paths/register -r ts-node/register src/native/main.ts",
    "lint": "eslint --ext .ts --max-warnings=0 src",
    "autoformat": "eslint --ext .ts --max-warnings=0 --fix src"
  },
  "devDependencies": {
    "@types/passport-jwt": "^4.0.1",
    "node-gyp": "^11.0.0",
    "node-addon-api": "^8.3.0",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=18.0.0 <19",
    "npm": "please-use-yarn",
    "yarn": ">= 1.17.3"
  },
  "pkg": {
    "scripts": "dist/main.js",
    "assets": [
      "node_modules/thread-stream/lib/worker.js",
      "node_modules/@nut-tree-fork"
    ],
    "targets": [
      "node18-win-x64"
    ]
  },
  "bin": "dist/main.js",
  "peerDependencies": {
    "@nut-tree-fork/libnut": "*"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^7.7.2",
    "@typescript-eslint/parser": "^7.7.2",
    "eslint": "^8.57.0",
    "eslint-find-rules": "^4.1.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-sonarjs": "^0.25.1",
    "eslint-plugin-unicorn": "^56.0.0",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/common": "^10.4.15",
    "@nestjs/core": "^10.4.15",
    "@nestjs/devtools-integration": "^0.1.6",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/schematics": "^10.2.3",
    "@nut-tree-fork/nut-js": "^4.2.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "nestjs-pino": "^4.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pino-http": "^10.3.0",
    "pino-pretty": "^13.0.0",
    "reflect-metadata": "^0.2.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
