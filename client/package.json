{
  "name": "l2",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:akoidan/l2.git",
  "author": "akoidan <deathangel908@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node -r tsconfig-paths/register -r ts-node/register src/main.ts",
    "prebuild": "nest build --clean && cp src/auth/public_key.pem build/auth/public_key.pem",
    "build": "pkg . --targets win --output app.exe",
    "wine": "wine app.exe"
  },
  "devDependencies": {
    "@types/passport-jwt": "^4.0.1",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.2"
  },
  "pkg": {
    "scripts": "build/main.js",
    "assets": [
      "node_modules/thread-stream/lib/worker.js",
      "node_modules/@nut-tree-fork/libnut-win32/build/Release/libnut.node"
    ],
    "targets": [
      "node18-win-x64"
    ]
  },
  "bin": "build/main.js",
  "dependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/common": "^10.4.15",
    "@nestjs/core": "^10.4.15",
    "@nestjs/devtools-integration": "^0.1.6",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/schematics": "^10.2.3",
    "@nut-tree-fork/nut-js": "^4.2.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "nestjs-pino": "^4.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pino-http": "^10.3.0",
    "pino-pretty": "^13.0.0",
    "reflect-metadata": "^0.2.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
