{
  "name": "l2",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:akoidan/l2.git",
  "author": "akoidan <deathangel908@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "rm -rf ./dist/ && node -r tsconfig-paths/register -r ts-node/register src/main.ts",
    "prebuild": "rm -rf ./dist/ && nest build && mkdir ./dist/native/win32 && cp ./src/native/win32/window.node ./dist/native/win32/window.node",
    "build": "pkg . --targets win --output app.exe",
    "wine": "wine app.exe",
    "win32": "node-gyp rebuild && cp ./build/Release/window.node ./src/native/window.node",
    "lint": "eslint --ext .ts --max-warnings=0 src",
    "autoformat": "eslint --ext .ts --max-warnings=0 --fix src",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nut-tree-fork/nut-js": "^4.2.2",
    "@types/cli-color": "^2.0.6",
    "@types/express": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^7.7.2",
    "@typescript-eslint/parser": "^7.7.2",
    "eslint": "^8.57.0",
    "eslint-find-rules": "^4.1.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-sonarjs": "^0.25.1",
    "eslint-plugin-unicorn": "^56.0.0",
    "node-addon-api": "^8.3.0",
    "patch-package": "^8.0.0",
    "tsconfig-paths": "^4.2.0",
    "node-gyp": "^11.0.0",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0 <19",
    "npm": "please-use-yarn",
    "yarn": ">= 1.17.3"
  },
  "pkg": {
    "scripts": "dist/main.js",
    "assets": [
      "node_modules/thread-stream/lib/worker.js",
      "node_modules/@nut-tree-fork"
    ],
    "targets": [
      "node18-win-x64"
    ]
  },
  "bin": "dist/main.js",
  "peerDependencies": {
    "@nut-tree-fork/libnut": "*"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.15",
    "@nestjs/core": "^10.4.15",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/schematics": "^10.2.3",
    "cli-color": "^2.0.4",
    "reflect-metadata": "^0.2.2",
    "zod": "^3.24.1"
  }
}
