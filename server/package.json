{
  "name": "l2",
  "version": "1.0.0",
  "main": "src/index.js",
  "repository": "git@github.com:akoidan/l2.git",
  "author": "akoidan <deathangel908@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./l2-win32-x64 && electron-packager . l2 --platform=win32 --arch=x64",
    "start": "rm -rf ./dist/ &&  electron ./src/index.js",
    "schema:json": "node -r tsconfig-paths/register -r ts-node/register src/config/generate-schema.ts",
    "lint": "eslint --ext .ts --max-warnings=0 src",
    "autoformat": "eslint --ext .ts --max-warnings=0 --fix src",
    "schema:doc": "zod2md --entry ./src/config/types/schema.ts --title 'Schema for config.jsonc' --output README.md",
    "test": "jest --coverage  --detectOpenHandles --testTimeout=10000"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.0.3",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^7.7.2",
    "@typescript-eslint/parser": "^7.7.2",
    "electron": "^30.0.6",
    "electron-packager": "^17.1.2",
    "eslint": "^8.57.0",
    "eslint-find-rules": "^4.1.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-sonarjs": "^0.25.1",
    "eslint-plugin-unicorn": "^56.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.4.5",
    "zod-to-json-schema": "^3.24.1",
    "zod2md": "^0.1.4",
    "@types/cli-color": "^2.0.6"
  },
  "engines": {
    "node": ">=18.0.0 <19",
    "npm": "please-use-yarn",
    "yarn": ">= 1.17.3"
  },
  "peerDependencies": {
    "@nut-tree-fork/libnut": "*"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.15",
    "@nestjs/core": "^10.4.15",
    "cli-color": "^2.0.4",
    "jsonc-parser": "^3.3.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "7.1.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.23.8"
  }
}
